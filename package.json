{
	"name": "wynnbot",
	"version": "0.0.1",
	"description": "Wynn Discord bot",
	"main": "src/index.js",
	"scripts": {
		"register": "node -r dotenv/config src/slash/register.js",
		"watch": "node -L --watch src",
		"start": "node src/index.js",
		"lint": "eslint --fix --ext .js, .",
		"format": "prettier --write \"src/**/*.js\""
	},
	"prettier": "@sapphire/prettier-config",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/alikuxac/wynn-bot.git"
	},
	"keywords": [
		"discord-bot",
		"wynnn",
		"sapphire",
		"sapphiredev"
	],
	"author": "Alikuxac",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/alikuxac/wynn-bot/issues"
	},
	"homepage": "https://github.com/alikuxac/wynn-bot#readme",
	"dependencies": {
		"@discordjs/collection": "^0.7.0",
		"@discordjs/rest": "^0.5.0",
		"@sapphire/discord-utilities": "^2.11.2",
		"@sapphire/discord.js-utilities": "latest",
		"@sapphire/fetch": "latest",
		"@sapphire/framework": "^2.5.1",
		"@sapphire/plugin-api": "^3.2.1",
		"@sapphire/plugin-editable-commands": "^1.2.0",
		"@sapphire/plugin-i18next": "^2.5.2",
		"@sapphire/plugin-logger": "latest",
		"@sapphire/plugin-subcommands": "latest",
		"@sapphire/time-utilities": "latest",
		"@sapphire/type": "^2.2.4",
		"@sapphire/utilities": "^3.6.2",
		"@skyra/editable-commands": "^2.1.2",
		"canvas": "^2.9.3",
		"discord-api-types": "^0.35.0",
		"discord.js": "^13.8.1",
		"dotenv": "^16.0.1",
		"lodash": "^4.17.21",
		"mongoose": "^6.4.0",
		"node-cron": "^3.0.1",
		"uuid": "^8.3.2",
		"winston-daily-rotate-file": "^4.7.1"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.18.2",
		"@sapphire/prettier-config": "^1.4.3",
		"eslint": "^8.18.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.7.1",
		"pretty-quick": "^3.1.3"
	},
	"lint-staged": {
		"*.js": "eslint --cache --fix"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged && pretty-quick --staged"
		}
	}
}
