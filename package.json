{
	"name": "wynnbot",
	"version": "0.0.1",
	"description": "Wynn Discord bot",
	"main": "src/index.js",
	"scripts": {
		"watch": "nodemon -L --watch src",
		"start": "nodemon src/index.js",
		"lint": "eslint --fix --ext .js, .",
		"format": "prettier --write \"src/**/*.js\""
	},
	"prettier": "@sapphire/prettier-config",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/alikuxac/wynn-bot.git"
	},
	"keywords": [
		"discord-bot",
		"wynnn",
		"sapphire",
		"sapphiredev"
	],
	"author": "Alikuxac",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/alikuxac/wynn-bot/issues"
	},
	"homepage": "https://github.com/alikuxac/wynn-bot#readme",
	"dependencies": {
		"@discordjs/collection": "^0.2.1",
		"@sapphire/discord-utilities": "^2.1.5",
		"@sapphire/discord.js-utilities": "next",
		"@sapphire/fetch": "next",
		"@sapphire/framework": "^2.0.0-next.71c18830.0",
		"@sapphire/plugin-api": "next",
		"@sapphire/plugin-editable-commands": "^0.1.0-next.9af3e85.0",
		"@sapphire/plugin-i18next": "^2.0.0-next.23809.0",
		"@sapphire/plugin-logger": "next",
		"@sapphire/plugin-subcommands": "next",
		"@sapphire/time-utilities": "next",
		"@sapphire/type": "^2.1.0",
		"@sapphire/utilities": "^2.0.1",
		"@skyra/editable-commands": "^2.1.0",
		"discord.js": "^13.1.0",
		"dotenv": "^10.0.0",
		"mongoose": "^6.0.8"
	},
	"devDependencies": {
		"@sapphire/prettier-config": "^1.1.6",
		"babel-eslint": "^10.1.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.2",
		"lint-staged": "^11.2.0",
		"nodemon": "^2.0.12",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.4.1",
		"pretty-quick": "^3.1.1"
	},
	"lint-staged": {
		"*.js": "eslint --cache --fix"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged && pretty-quick --staged"
		}
	}
}
